<!--pages/history_page/history_page.wxml-->
<block wx:for="{{showrecordList}}" wx:key="item">
  <view class='container' 
  bindtap='detail'
  data-id='{{item.id}}'>

    <view class='item-list'>
      <text class='time'>{{item.browDate}}</text>
      <text class='title'>{{item.title}}</text>
    </view>
    
  </view>
</block>
