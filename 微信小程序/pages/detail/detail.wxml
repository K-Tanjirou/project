<view class='container'>
  <text class="title">{{title}}</text>
  <text class='date'>日期：{{date}}</text>
  <text class="author">作者：{{author}}</text>
  <text class="lwContent">{{lwContent}}</text>
  <button class="cu-btn round bg-blue" id="button" bindtap='ToDownLoad'
    data-title="{{title}}"
    data-date="{{date}}"
    data-author="{{author}}"
    data-lwid="{{lwid}}"
  >下载</button>
  <loading hidden="{{loadingHidden}}">
    加载中...
  </loading>
</view>

